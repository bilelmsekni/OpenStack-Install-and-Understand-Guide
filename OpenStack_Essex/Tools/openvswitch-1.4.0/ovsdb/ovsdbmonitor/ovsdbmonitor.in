import sys, traceback
from pprint import pprint
from OVEApp import *

app = OVEApp()
try:
    retVal = app.enter()
except Exception, e:
    print str(e)
    try:
        trace = traceback.format_tb(sys.exc_info()[2])
    except:
        trace = ['Traceback not available']
    print("".join(trace))
    retVal = 1

sys.exit(retVal)
